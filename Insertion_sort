def insertion_sort(a)
    for i in 1...(a.length)
        j=i
        while j>0
            if a[j-1]>a[j]
                temp=a[j]
                a[j]=a[j-1]
                a[j-1]=temp
            else
              break
            end
            j=j-1
        end
    end
   return a 
end

array=[1,300,224,232243,22,446,765]
print ("#{insertion_sort(array)} ")
